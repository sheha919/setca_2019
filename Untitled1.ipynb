    "def calculate_distances(atom1,atom2):\n",
    "    x_distance=atom1[0]-atom2[0]\n",
    "    y_distance=atom1[1]-atom2[1]\n",
    "    z_distance=atom1[2]-atom2[2]\n",
    "    distance=numpy.sqrt(x_distance**2 + y_distance**2 + z_distance**2)\n",
    "    return distance"
   ]
  }
